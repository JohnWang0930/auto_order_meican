{
    "presets": [
        [
            "@babel/env",
            {
                "targets": {
                    "edge": "17",
                    "firefox": "60",
                    "chrome": "67",
                    "safari": "11.1"
                },
                "useBuiltIns": "usage",
                "corejs": "2"
            }
        ]
    ],
    "plugins": [
        "@babel/plugin-transform-react-jsx",
        [
            "import", 
            {
                "libraryName": "antd",
                "style": "css"
              }
        ]
    ]
}